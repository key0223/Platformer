  iseldaDialogues:
  - nodeId: Iselda_first_meet
    dialogueText: "\uC9C0\uB3C4\uB97C \uC0AC\uB7EC \uC654\uB098\uC694? \uC0AC\uC2E4
      \uB2F9\uC2E0\uC774 \uB9CC\uB098\uC57C \uD558\uB294 \uAC83\uC740 \uB0B4 \uB0A8\uD3B8\uC774\uC9C0\uB9CC,
      \uC5B4\uCA4C\uACA0\uC5B4\uC694! \uADF8\uB294 \uB2E4\uC2DC \uB3D9\uAD74\uB85C
      \uB0B4\uB824\uAC14\uC5B4\uC694.#b\uADF8\uB294 \uAC00\uAC8C\uC5D0 \uC0C8 \uC9C0\uB3C4\uB97C
      \uAC16\uB2E4 \uC8FC\uB7EC \uAC00\uB054\uC529 \uC62C\uB77C\uC624\uC9C0\uB9CC,
      \uC5EC\uAE30 \uC704\uC5D0\uC11C \uC880 \uB354 \uC2DC\uAC04\uC744 \uBCF4\uB0C8\uC73C\uBA74
      \uC88B\uACA0\uC5B4\uC694. \uB098\uB294 \uC7A5\uC0AC\uC5D0\uB294 \uBCC4\uB85C
      \uAD00\uC2EC\uC774 \uC5C6\uC5B4\uC694.#b\uC790, \uB0B4\uAC00 \uD314\uB824\uB294
      \uAC83\uB4E4\uC744 \uD55C \uBC88 \uBCF4\uC138\uC694. "
    conditionKey: 
    hasFollowingDialogue: 0
    followingDialogueId: 
  elderbugDialogues:
  - nodeId: Elderbug_first_meet
    dialogueText: "\uC774\uBCF4\uC2DC\uC624, \uC5EC\uD589\uC790. \uC5EC\uD589\uC790\uB97C
      \uB9DE\uC774\uD560 \uC218 \uC788\uB294 \uC774\uB294 \uB098\uBC16\uC5D0 \uC548
      \uB0A8\uC740 \uAC83 \uAC19\uC18C. \uBCF4\uB2E4\uC2DC\uD53C \uB9C8\uC744\uC774
      \uC870\uC6A9\uD574\uC84C\uC18C.\r\n\uB2E4\uB978 \uC8FC\uBBFC\uB4E4\uC740 \uBAA8\uB450
      \uC0AC\uB77C\uC84C\uC18C. \uC800 \uC6B0\uBB3C\uC744 \uD1B5\uD574 \uC544\uB798\uB85C,
      \uD558\uB098\uC529, \uC544\uB798\uC5D0 \uC788\uB294 \uB3D9\uAD74\uB85C."
    conditionKey: 
    hasFollowingDialogue: 1
    followingDialogueId: Elderbug_first_meet_following
  - nodeId: Elderbug_passingBy
    dialogueText: "\uC624! \uB3CC\uC544\uC654\uAD70! \uB0A0 \uBC14\uB85C \uC9C0\uB098\uCE58\uAE38\uB798
      \uC544\uB9C8\uB3C4 \uB0B4\uAC00 \uB9C8\uC744\uACFC \uD568\uAED8 \uC0AC\uB77C\uC9C4
      \uAC74\uAC00 \uD558\uACE0 \uC0DD\uAC01\uD588\uC18C."
    conditionKey: 
    hasFollowingDialogue: 1
    followingDialogueId: Elderbug_first_meet_following
  - nodeId: Elderbug_first_meet_following
    dialogueText: "\uC6B0\uB9AC \uB9C8\uC744 \uC544\uB798\uC5D0\uB294 \uC704\uB300\uD55C
      \uC655\uAD6D\uC774 \uC788\uC5C8\uC18C. \uD3D0\uD5C8\uAC00 \uB41C\uC9C0 \uC624\uB798\uC9C0\uB9CC,
      \uC544\uC9C1\uB3C4 \uADF8 \uAE4A\uC740 \uACF3\uC73C\uB85C \uB9CE\uC740 \uC774\uB4E4\uC774
      \uB0B4\uB824\uAC00\uC624.#b\r\n\uBD80,\uC601\uAD11, \uAE68\uB2EC\uC74C... \uC800
      \uBC11\uC5D0\uC11C \uC6D0\uD558\uB294 \uAC83\uC744 \uCC3E\uC744 \uC218 \uC788\uC744
      \uAC70\uB77C \uC0DD\uAC01\uD558\uB294 \uB4EF \uD558\uC624. \uB2F9\uC2E0\uB3C4
      \uAFC8\uC744 \uC887\uC544 \uC800 \uC544\uB798\uB85C \uB0B4\uB824\uAC00\uACA0\uC9C0.#b\uADF8\uB798\uB3C4
      \uC870\uC2EC\uD558\uC624. \uC5ED\uACA8\uC6B4 \uACF5\uAE30\uAC00 \uADF8\uC637\uC744
      \uCC44\uC6B0\uACE0 \uC788\uC18C. \uBC8C\uB808\uB4E4\uC774 \uBBF8\uCCD0\uAC00\uACE0
      \uC5EC\uD589\uC790\uB4E4\uC740 \uAE30\uC5B5\uC744 \uBE7C\uC557\uAE30\uACE0...#b\uC544\uB9C8\uB3C4
      \uAFC8\uC740 \uADF8\uB807\uAC8C \uB300\uB2E8\uD55C \uAC83\uC774 \uC544\uB2D0\uC9C0\uB3C4..."
    conditionKey: 
    hasFollowingDialogue: 1
    followingDialogueId: Elderbug_talk_1
  - nodeId: Elderbug_talk_1
    dialogueText: "\uC655\uAD6D\uC774 \uC790\uC2E0\uC758 \uC695\uB9DD\uC744 \uCDA9\uC871\uC2DC\uD0A4\uAE30\uB97C
      \uBC14\uB77C\uBA70 \uB9CE\uC740 \uC774\uB4E4\uC774 \uC654\uC5C8\uC18C.#b\uD55C
      \uB54C\uB294 \uC2E0\uC131\uB465\uC9C0\uB77C\uACE0 \uBD88\uB838\uC9C0. \uC544\uB9C8
      \uADF8\uACF3\uC740 \uBCF4\uBB3C\uACFC \uBE44\uBC00\uB85C \uAC00\uB4DD \uCC2C
      \uC704\uB300\uD55C \uC655\uAD6D\uC774 \uC788\uC5C8\uC744 \uAC70\uC694.#b\uD760.
      \uC774\uC81C\uB294 \uAD34\uBB3C\uACFC \uAD11\uAE30\uB85C \uAC00\uB4DD\uD55C
      \uBB34\uB364\uC77C \uBFD0\uC774\uC624.#b\uACB0\uAD6D \uBAA8\uB4E0 \uAC83\uC740
      \uC0AC\uB77C\uC9C0\uB294 \uAC70\uC9C0."
    conditionKey: 
    hasFollowingDialogue: 1
    followingDialogueId: Elderbug_talk_2
  - nodeId: Elderbug_talk_2
    dialogueText: "\uD53C\uACE4\uD558\uC624? \uC800 \uC758\uC790\uAC00 \uC1E0\uB85C
      \uB418\uC5B4 \uC788\uC744\uC9C0 \uBAB0\uB77C\uB3C4 \uAF64 \uD3B8\uD558\uC624.
      \uC544\uB798\uB85C \uB0B4\uB824\uAC00\uAE30 \uC804\uC5D0 \uC0DD\uAC01\uC744
      \uC815\uB9AC\uD558\uAE30\uC5D0 \uC548\uC131\uB9DE\uCDA4\uC774\uC624.#b\uB098\uB3C4
      \uB204\uAC00 \uC606\uC5D0 \uC788\uC73C\uBA74 \uC88B\uACE0. \uB9D0\uC774 \uB9CE\uC740
      \uD3B8\uC740 \uC544\uB2CC \uAC83 \uAC19\uC9C0\uB9CC."
    conditionKey: 
